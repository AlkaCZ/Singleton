<Window x:Class="Singleton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Singleton"
        mc:Ignorable="d"
        Title="ShowDictionary" Height="300" Width="400
        ">
    <Grid>
        <TextBox x:Name="tbOut" HorizontalAlignment="Center" Height="150" TextWrapping="Wrap" Text="Empty" VerticalAlignment="Top" Width="350"/>
        <Button x:Name="StartButton" Click="Start_Click" Content="Start" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="75" Margin="0,0,0,50"></Button>
    </Grid>
</Window>
